{
  "name": "intellectus-client",
  "version": "0.0.1",
  "description": "Client side for Intellectus",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run test",
    "webpack:watch": "webpack-dev-server --mode=development --config webpack.dev.config.js",
    "start": "npm run sass && run-p sass:watch webpack:watch",
    "build": "webpack --mode=production --config webpack.prod.config.js",
    "build:server": "npm run build && npm run sass && ncp ./dist ../server/public/ && ncp ./public ../server/public",
    "lint": "eslint src --ext .ts,.tsx",
    "sass": "node-sass scss/main.scss dist/styles.css",
    "sass:watch": "node-sass --watch scss/main.scss dist/styles.css",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/777PolarFox777/Intellectus.git"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "author": "NoBrains",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/777PolarFox777/Intellectus/issues"
  },
  "homepage": "https://github.com/777PolarFox777/Intellectus#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/history": "^4.7.3",
    "@types/lodash": "^4.14.141",
    "@types/react-dom": "^16.9.1",
    "@types/react-router-dom": "^4.3.5",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "circular-dependency-plugin": "^5.2.0",
    "cypress": "^3.4.1",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prefer-arrow": "^1.1.6",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "ncp": "^2.0.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^6.2.0",
    "tslib": "^1.10.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  }
}
