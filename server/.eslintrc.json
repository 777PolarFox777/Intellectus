{
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb"
  ],
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error", "info"] }],
    "semi": "error",
    "function-paren-newline": ["error", "multiline"],
    "max-params": "error",
    "multiline-comment-style": "error"
  }
}
